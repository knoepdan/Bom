{
    "name": "react-typescript-boilerplate",
    "version": "1.0.0",
    "private": true,
    "description": "A frontend boilerplate with React and Typescript.",
    "homepage": "",
    "keywords": [
        "react",
        "typescript",
        "boilerplate"
    ],
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server --mode development --hot --progress --color --port 3000 --open",
        "dev": "webpack --config=webpack.dev.js --watch",
        "build": "webpack --config=webpack.prod.js",
        "CopyExternalConfigToBuild": "xcopy /Y src\\externalConfig.prod.json \\..\\..\\wwwRoot\\app\\externalConfig.json*",
        "AdaptForProduction": "node scripts/adaptForproduction.js BomUI true",
        "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
        "lintfix": "npx eslint  src/**/**/* --fix",
        "lint": "npx eslint  src/**/**/* --fix-dry-run",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.14.8",
        "@babel/preset-react": "^7.14.5",
        "@types/enzyme": "^3.10.4",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^26.0.8",
        "@types/node": "^14.0.27",
        "@types/react": "^16.9.44",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.5",
        "@types/webpack": "^4.41.21",
        "@types/webpack-env": "1.15.2",
        "@typescript-eslint/eslint-plugin": "^3.7.1",
        "@typescript-eslint/parser": "^3.7.1",
        "babel-loader": "^8.0.6",
        "browser-sync": "^2.27.4",
        "browser-sync-webpack-plugin": "^2.2.2",
        "colors": "^1.4.0",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^6.2.0",
        "html-webpack-plugin": "^5.2.6",
        "mini-css-extract-plugin": "^2.1.0",
        "node-sass": "^6.0.1",
        "redux-devtools-extension": "^2.13.8",
        "resx-json-typescript-converter": "^1.0.1",
        "sass-loader": "^12.1.0",
        "style-loader": "^3.2.1",
        "ts-loader": "^9.2.3",
        "typescript": "^4.3.5",
        "webpack": "^5.46.0",
        "webpack-cli": "^4.7.2",
        "webpack-merge": "^5.8.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.5.0",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.5",
        "jest": "^26.2.2",
        "jest-css-modules": "^2.1.0",
        "prettier": "^2.0.5",
        "react-hot-loader": "^4.12.21",
        "ts-jest": "^26.1.4"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.30",
        "@fortawesome/free-solid-svg-icons": "^5.14.0",
        "@fortawesome/react-fontawesome": "^0.1.11",
        "@hookstate/core": "^3.0.1",
        "classnames": "^2.2.6",
        "primeicons": "^2.0.0",
        "primereact": "^4.2.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0"
    }
}
